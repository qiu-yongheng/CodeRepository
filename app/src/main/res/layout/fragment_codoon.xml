<?xml version="1.0" encoding="utf-8"?>
<com.qyh.coderepository.menu.view.codoon.MyFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/v_slide_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.qyh.coderepository.menu.view.codoon.MyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.qyh.coderepository.menu.view.codoon.MyImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@mipmap/map"/>

            <ImageView
                android:src="@mipmap/sport_data"
                android:scaleType="fitXY"
                android:layout_width="match_parent"
                android:layout_height="2500dp"/>

        </LinearLayout>
    </com.qyh.coderepository.menu.view.codoon.MyScrollView>

    <LinearLayout
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <android.support.v7.widget.Toolbar
            android:id="@+id/tool_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/ToolbarTheme"/>

        <com.qyh.coderepository.menu.view.codoon.TabIndicatorView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>


</com.qyh.coderepository.menu.view.codoon.MyFrameLayout>